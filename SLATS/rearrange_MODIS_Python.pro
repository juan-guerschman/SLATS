          where_value = where(DATA.field3 eq DATE_String AND $
                            SiteLat_Str[sitio] eq Latitude_Str AND $
                            SiteLon_Str[sitio] eq Longitude_Str AND $
                            band eq  uniq_band[element], count)
          if count eq 1 then value = DATA.field6[where_value] ELSE value = -9999
          
          OutLine = OutLine + ',' + String(value)
          
        EndFor ; element  
        
        OutLine = StrCompress(OutLine, /Remove_all)
        
        PrintF, 1, OutLine
      
      endfor   ;composite
    endfor  ;year
  endfor    ;sitio 

end

